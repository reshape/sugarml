{
  "name": "sugarml",
  "description": "basic parser for whitespace-significant html",
  "version": "0.5.0",
  "author": "Jeff Escalante",
  "ava": {
    "verbose": "true"
  },
  "bugs": "https://github.com/reshape/sugarml/issues",
  "devDependencies": {
    "ava": "^0.17.0",
    "coveralls": "^2.11.12",
    "matcha": "^0.7.0",
    "nyc": "^10.0.0",
    "pug": "^2.0.0-beta6",
    "reshape": "^0.4.0",
    "snazzy": "^6.0.0",
    "standard": "10.0.0"
  },
  "engines": {
    "node": ">= 6.0.0"
  },
  "homepage": "https://github.com/reshape/sugarml",
  "license": "MIT",
  "main": "lib",
  "repository": "reshape/sugarml",
  "scripts": {
    "test": "npm run lint && nyc ava",
    "lint": "standard | snazzy",
    "coverage": "npm test && nyc report --reporter=html && open ./coverage/index.html",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "bench": "matcha"
  },
  "dependencies": {
    "code-frame": "^5.0.0"
  }
}
